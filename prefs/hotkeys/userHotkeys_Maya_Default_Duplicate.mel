//Maya Preference 2020 (Release 1)
//
//

//
// The hotkey set
//
hotkeySet -source "Maya_Default" -current Maya_Default_Duplicate;
//
// The hotkey contexts and hotkeys
//
hotkey -keyShortcut "w" -alt -sht -name ("PinTranslateNameCommand");
hotkey -keyShortcut "m" -releaseName ("ModifyOpacityReleaseNameCommand");
hotkey -keyShortcut "m" -name ("ModifyOpacityPressNameCommand");
hotkey -keyShortcut "r" -alt -sht -name ("FrameSelectedTimelineNameCommand");
hotkey -keyShortcut "0" -ctl -name ("GraphEditorNameCommand");
hotkey -keyShortcut "s" -alt -sht -name ("KeyRangeNameCommand");
hotkey -keyShortcut "/" -releaseName ("TimeDraggerToolDeactivateNameCommand");
hotkey -keyShortcut "h" -alt -sht -name ("SelectHierarchyNameCommand");
hotkey -keyShortcut "/" -name ("TimeDraggerToolActivateNameCommand");
hotkey -keyShortcut "e" -alt -sht -name ("PinRotateNameCommand");
hotkey -keyShortcut "v" -alt -sht -name ("Playback60SlowNameCommand");
hotkey -keyShortcut "v" -alt -name ("PlaybackNormalNameCommand");
hotkey -keyShortcut "g" -alt -sht -name ("TrajectoryGuideNameCommand");
hotkeyCtx -type "Editor" -addClient "nodeEditorPanel";
hotkeyCtx -type "Editor" -addClient "hyperShadePanel";
hotkeyCtx -type "Editor" -addClient "outlinerPanel";
hotkeyCtx -type "Editor" -addClient "graphEditor";
hotkeyCtx -type "Editor" -addClient "polyTexturePlacementPanel";
hotkeyCtx -type "Editor" -addClient "shapePanel";
hotkeyCtx -type "Editor" -addClient "profilerPanel";
hotkeyCtx -type "Editor" -addClient "timeEditorPanel";
hotkey -keyShortcut "/" -releaseName ("TimeEditorToggleTimeCursorReleaseNameCommand");
hotkey -keyShortcut "/" -name ("TimeEditorToggleTimeCursorPressNameCommand");
hotkeyCtx -type "Editor" -addClient "hyperGraphPanel";
hotkeyCtx -type "Editor" -addClient "posePanel";
hotkeyCtx -type "Tool" -addClient "xgmIGBrush";
hotkeyCtx -type "Tool" -addClient "SymmetrizeUVBrush";
hotkeyCtx -type "Tool" -addClient "polyCutUV";
hotkeyCtx -type "Tool" -addClient "texSculptCacheContext";
hotkeyCtx -type "Tool" -addClient "texCutContext";
hotkeyCtx -type "Tool" -addClient "sculptMeshCache";
hotkeyCtx -type "Tool" -addClient "Unfold3DBrush";
